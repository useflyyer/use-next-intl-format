{
  "name": "use-next-intl-format",
  "version": "1.2.1",
  "description": "React Hook for creating memoized native instances of Intl for Next.js i18n",
  "author": "lopezjurip",
  "license": "MIT",
  "repository": "flayyer/use-next-intl-format",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "sideEffects": false,
  "engines": {
    "node": ">=12",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rollup -c -w",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "dequal": "^2.0.2"
  },
  "peerDependencies": {
    "next": ">=10.0.0",
    "react": ">=16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/runtime": "^7.14.6",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@rollup/plugin-url": "^6.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.2",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.52.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  }
}
